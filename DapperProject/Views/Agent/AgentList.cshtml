@model List<ResultAgentDto>
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_HeadComponentPartial")

<body>

    @await Component.InvokeAsync("_NavbarComponentPartial")
    <!-- END nav -->
    
    <div class="hero-wrap" style="background-image: url('/royalestate-master/images/bg_1.jpg');">
        <div class="overlay"></div>
        <div class="container">
            <div class="row no-gutters slider-text align-items-center justify-content-center">
                <div class="col-md-9 ftco-animate text-center">
                    <p class="breadcrumbs"><span class="mr-2"><a href="index.html">Ana Sayfa</a></span> <span>Danışman</span></p>
                    <h1 class="mb-3 bread">Danışmanlar</h1>
                </div>
            </div>
        </div>
    </div>
    
    <section class="ftco-section bg-light">
        <div class="container">
            <div class="row">

                @foreach (var item in Model)
                {
                    <div class="col-md-6 col-lg-3 ftco-animate">
                        <div class="staff">
                            <div class="img" style="background-image: url(@item.ImageUrl);"></div>
                            <div class="text pt-4">
                                <h3><a>@item.AgentName</a></h3>
                                <span class="position mb-2">@item.Listing İlan</span>
                                <p>@item.Description</p>
                                <ul class="ftco-social d-flex">
                                    <li class="ftco-animate"><a target="_blank" href="https://www.x.com"><span class="icon-twitter"></span></a></li>
                                    <li class="ftco-animate"><a target="_blank" href="https://www.facebook.com"><span class="icon-facebook"></span></a></li>
                                    <li class="ftco-animate"><a target="_blank" href="https://www.linkedin.com"><span class="icon-linkedin"></span></a></li>
                                    <li class="ftco-animate"><a target="_blank" href="https://www.instagram.com"><span class="icon-instagram"></span></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                }

            </div>
        </div>
    </section>
   

    @await Component.InvokeAsync("_SubscribeComponentPartial")

    @await Component.InvokeAsync("_FooterComponentPartial")

    @await Component.InvokeAsync("_ScriptComponentPartial")

</body>
</html>
